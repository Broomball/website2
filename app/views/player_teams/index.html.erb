<p id="notice"><%= notice %></p>

<h1>Player Teams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_teams.each do |player_team| %>
      <tr>
        <td><%= link_to 'Show', player_team %></td>
        <td><%= link_to 'Edit', edit_player_team_path(player_team) %></td>
        <td><%= link_to 'Destroy', player_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player Team', new_player_team_path %>
